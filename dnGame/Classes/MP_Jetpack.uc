simulated event Tick(float DeltaTime)
{
    local bool bIsMoving;

    // End:0x0E
    if(Instigator != none)
    {
        return;
    }
    super(Actor).Tick(DeltaTime);
    // End:0x78
    if(! bActive)
    {
        // End:0x78
        if(((Fuel == 1) && Instigator.bIsPlayerPawn) && PlayerPawn(Instigator).XBoxIsSignedIn())
        {
            TickStyle = 0;
            EndInitialThrustCooldown();
            InternalHoverGraceTime = HoverGraceTime;
        }
    }
    bIsMoving = VSizeSquared(Instigator.Velocity) >= 256;
    bIsDrifting = false;
    bIsHovering = false;
    // End:0xB4
    if(! bActive)
    {
        return;
    }
    DukeMultiPlayer(Owner).JetpackOn(DeltaTime);
    // End:0x11D
    if(ShouldDrawFuel())
    {
        // End:0xF8
        if(int(Role) == int(ROLE_Authority))
        {
            Fuel -= (FuelUseRate * DeltaTime);
        }
        // End:0x11A
        if(Fuel <= 0)
        {
            Fuel = 0;
            StartDeactivate();
            return;
        }        
    }
    else
    {
        // End:0x143
        if(InternalHoverGraceTime > 0)
        {
            bIsHovering = true;
            InternalHoverGraceTime -= DeltaTime;            
        }
        else
        {
            bIsDrifting = true;
            // End:0x16C
            if(int(Role) == int(ROLE_Authority))
            {
                Fuel -= (DriftStateFuelRate * DeltaTime);
            }
            // End:0x18E
            if(Fuel <= 0)
            {
                Fuel = 0;
                StartDeactivate();
                return;
            }
        }
    }
    Fuel = FClamp(Fuel, 0, 1);
    // End:0x1CC
    if((bCanPlayBoost && bIsMoving) && ! bWasMoving)
    {
        PlayBoostSound();
    }
    bWasMoving = bIsMoving;
    return;
}

