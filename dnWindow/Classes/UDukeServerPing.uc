function string ParseReply(string Text, string Key)
{
    local int i;
    local string temp;

    i = InStr(Text, ("\\" $ Key) $ "\\");
    temp = Mid(Text, (i + Len(Key)) + 2);
    return Left(temp, InStr(temp, "\\"));
    return;
}

function AddMutatorRules(string MutatorsIn)
{
    local int NextMut, NumMutators;

    J0x00:
    // End:0x8E [Loop If]
    if(NextMut != -1)
    {
        ++ NumMutators;
        NextMut = InStr(MutatorsIn, ",");
        // End:0x54
        if(NextMut == -1)
        {
            AddRule(MutatorsText $ string(NumMutators), MutatorsIn);
            return;            
        }
        else
        {
            AddRule(MutatorsText @ string(NumMutators), Left(MutatorsIn, NextMut));
            MutatorsIn = Mid(MutatorsIn, NextMut + 1);
        }
        // [Loop Continue]
        goto J0x00;
    }
    return;
}

