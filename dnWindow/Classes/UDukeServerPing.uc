function AddRule(string Rule, string Value)
{
    local UDukeRulesList RulesList;

    ValidateServer();
    RulesList = UDukeRulesList(Server.RulesList.Next);
    J0x2A:

    // End:0x6F [Loop If]
    if(RulesList == none)
    {
        // End:0x52
        if(RulesList.Rule == Rule)
