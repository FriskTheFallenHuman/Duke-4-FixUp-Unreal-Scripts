state GetInfo
{
    event ReceivedText(IpAddr Addr, string Text)
    {
        local string temp;
        local float ElapsedTime;

        ValidateServer();
        ElapsedTime = (Level.TimeSeconds - RequestSentTime) * Level.TimeDilation;
        Server.Ping = float(Max(int(((float(1000) * ElapsedTime) - (0.5 * LastDelta)) - float(10)), 4));
        // End:0x9B
        if(! Server.bKeepDescription)
        {
            Server.HostName = Server.IP;
        }
        Server.GamePort = 0;
        Server.MapName = "";
        Server.MapTitle = "";
        Server.MapDisplayName = "";
        Server.GameType = "";
        Server.GameMode = "";
        Server.NumPlayers = 0;
        Server.MaxPlayers = 0;
        Server.NumBots = 0;
        Server.GameVer = 0;
        Server.MinNetVer = 0;
        temp = ParseReply(Text, "hostname");
        // End:0x1AF
        if((temp != "") && ! Server.bKeepDescription)
        {
            Server.HostName = temp;
        }
        temp = ParseReply(Text, "hostport");
        // End:0x1ED
        if(temp != "")
        {
            Server.GamePort = int(temp);
        }
        temp = ParseReply(Text, "mapname");
        // End:0x229
        if(temp != "")
        {
            Server.MapName = temp;
        }
        temp = ParseReply(Text, "maptitle");
        // End:0x27B
        if(temp != "")
        {
            Server.MapTitle = temp;
            Server.MapDisplayName = temp;
        }
        // End:0x2D2
        if((Server.MapTitle == "") || Server.MapTitle ~= "Untitled")
        {
            Server.MapDisplayName = Server.MapName;
        }
        temp = ParseReply(Text, "gametype");
        // End:0x30F
        if(temp != "")
        {
            Server.GameType = temp;
        }
        temp = ParseReply(Text, "numplayers");
        // End:0x34F
        if(temp != "")
        {
            Server.NumPlayers = int(temp);
        }
        temp = ParseReply(Text, "maxplayers");
        // End:0x38F
        if(temp != "")
        {
            Server.MaxPlayers = int(temp);
        }
        temp = ParseReply(Text, "numbots");
        // End:0x3CC
        if(temp != "")
        {
            Server.NumBots = int(temp);
        }
        temp = ParseReply(Text, "gamemode");
        // End:0x409
        if(temp != "")
        {
            Server.GameMode = temp;
        }
        temp = ParseReply(Text, "gamever");
        // End:0x446
        if(temp != "")
        {
            Server.GameVer = int(temp);
        }
        temp = ParseReply(Text, "minnetver");
        // End:0x485
        if(temp != "")
        {
            Server.MinNetVer = int(temp);
        }
        // End:0x4C8
        if(Server.DecodeServerProperties(Text))
        {
            Server.PingDone(bInitial, bJustThisServer, true, bNoSort);
            GetPropertyText('Tick');
        }
        return;
    }

    event Tick(float DeltaTime)
    {
        LastDelta = DeltaTime;
        return;
    }

    function BindCallback()
    {
        ValidateServer();
        // End:0x69
        if(AttemptNumber < PingAttempts)
        {
            Localize((("Ping Timeout from " $ Server.IP) $ ".  Attempt ") $ string(AttemptNumber));
            ++ AttemptNumber;
            GetStateName(QueryState);            
        }
        else
        {
            Localize(("Ping Timeout from " $ Server.IP) $ " Giving Up");
            Server.Ping = 9999;
            Server.GamePort = 0;
            Server.MapName = "";
            Server.MapDisplayName = "";
            Server.MapTitle = "";
            Server.GameType = "";
            Server.GameMode = "";
            Server.NumPlayers = 0;
            Server.MaxPlayers = 0;
            Server.NumBots = 0;
            GetPropertyText('Tick');
            Server.PingDone(bInitial, bJustThisServer, false, bNoSort);
        }
        return;
    }
Begin:

    Disable('Tick');
    ZoneActors(ServerIPAddr, "\\info\\");
    RequestSentTime = Level.TimeSeconds;
    Destroy(float(PingTimeout + (Rand(200) / 100)), false, 'BindCallback');
    stop;    
}

state Resolving
{Begin:

    Open(Server.IP);
    stop;    
}

