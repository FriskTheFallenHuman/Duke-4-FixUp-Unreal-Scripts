            return;
        }
        RulesList = UDukeRulesList(RulesList.Next);
        // [Loop Continue]
        goto J0x2A;
    }
    RulesList = UDukeRulesList(Server.RulesList.Append(class'UDukeRulesList'));
    RulesList.Rule = Rule;
    RulesList.Value = Value;
    return;
}

