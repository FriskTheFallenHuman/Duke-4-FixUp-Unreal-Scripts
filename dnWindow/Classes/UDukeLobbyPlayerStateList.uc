function string getMaxString(string PlayerName)
{
    local int strLength;

    strLength = Len(PlayerName);
    // End:0x3E
    if(strLength > 16)
    {
        PlayerName = Left(PlayerName, 16);
        PlayerName = PlayerName $ "... ";
    }
    return PlayerName;
    return;
}

