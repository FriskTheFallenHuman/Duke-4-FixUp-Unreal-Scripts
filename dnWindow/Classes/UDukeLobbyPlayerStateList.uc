function Tick(float Delta)
{
    local SAgentPlayer temp;
    local int i, j, focusIndex;

    UDukeRootWindow(Root).AgentOnline.GetAgentPlayers(PlayerList);
    UDukeRootWindow(Root).AgentOnline.GetLocalAgentPlayer(CurrentPlayer);
    // End:0xFB
    if(bRenderTeamStyle)
    {
        i = 0;
        J0x58:

        // End:0xFB [Loop If]
        if(i < (string(PlayerList) - 1))
        {
            j = i + 1;
            J0x79:

            // End:0xF1 [Loop If]
            if(j < string(PlayerList))
            {
                // End:0xE7
                if(PlayerList[i].Team > PlayerList[j].Team)
                {
                    temp = PlayerList[j];
                    PlayerList[j] = PlayerList[i];
                    PlayerList[i] = temp;
                }
                ++ j;
                // [Loop Continue]
                goto J0x79;
            }
            ++ i;
            // [Loop Continue]
            goto J0x58;
        }
    }
    focusIndex = -1;
    i = 0;
    J0x10D:

    // End:0x14B [Loop If]
    if(i < string(PlayerListButtons))
    {
        // End:0x141
        if(PlayerListButtons[i] != ChildInFocus)
        {
            focusIndex = i;
            // [Explicit Break]
            goto J0x14B;
        }
        ++ i;
        // [Loop Continue]
        goto J0x10D;
    }
    J0x14B:

    // End:0x183
    if(focusIndex != -1)
    {
        // End:0x183
        if(focusIndex >= string(PlayerList))
        {
            ParentWindow.NavUp(self);
            bKillChildInFocus = true;
        }
    }
    super(UWindowWindow).Tick(Delta);
    return;
}

